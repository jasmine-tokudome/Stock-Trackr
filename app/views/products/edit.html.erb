<%= @product_count %>

<table>

  <thead>
    <tr>
      <th>id</th>
      <th>name</th>
      <th>category</th>
      <th>unavailable_delivery_ids</th>
      <th>without_shipping</th>
    </tr>
  </thead>
  <tbody>
    <% @products["products"].each do |product| %>
      <tr>
        <td><%= product["id"] %></td>
        <td><%= product["name"] %></td>
        <td><%= product["category"] %></td>
        <td><%= product["unavailable_delivery_ids"] %></td>
        <td><%= product["without_shipping"] %></td>
      </tr>
    <% end %>
  </tbody>
<table>